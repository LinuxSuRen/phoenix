<autotest>
	<engine driver="chrome" timeout="10" fullScreen="false" width="1024" height="768">
	</engine>
	<pages>
		<page class="org.suren.autotest.web.framework.baidu.BaiduHomePage"
			url="https://www.baidu.com/s?wd=surenpi"
			dataSource="xmlDataSource">
			<field name="toLoginBut" byName="tj_login" type="button" />
			<field name="loginBut" byId="TANGRAM__PSP_8__submit" type="button" strategy="zone">
				<locators>
					<locator type="byId" value="head" />
					<locator type="byId" value="TANGRAM__PSP_8__submit" />
				</locators>
			</field>
			<field name="userNameText" byId="TANGRAM__PSP_8__userName" byName="username" type="text" strategy="priority" />
			<field name="passwdText" byName="password" type="text" />
		</page>
	</pages>
	<dataSources>
		<dataSource name="xmlDataSource" type="xml_data_source"
			resource="dataSource/baidu_website_datasource.xml"></dataSource>
	</dataSources>
</autotest>